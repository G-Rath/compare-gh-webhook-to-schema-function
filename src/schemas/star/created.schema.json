{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "star$created",
  "type": "object",
  "required": [
    "action",
    "starred_at",
    "repository",
    "sender"
  ],
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "created"
      ]
    },
    "starred_at": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string"
        }
      ]
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    },
    "organization": {
      "oneOf": [
        {
          "$ref": "common/organization.schema.json"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "additionalProperties": false,
  "title": "star created event"
}